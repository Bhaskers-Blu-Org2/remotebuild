<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <ProjectGuid>{BE8A7712-5551-4E7B-9629-03D4DFAFE99B}</ProjectGuid>
    <ProjectHome>$(TACO_ROOT)\src\$(MSBuildProjectName)</ProjectHome>
